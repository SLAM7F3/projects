	rm -f *.a *.o 

alirtclean:

	cd $(MAIN_ALIRT_DIR) ; \
	rm -f *.a *.o

	cd $(ASTROGEO_DIR) ; \
	rm -f *.a *.o
	cd $(COLOR_DIR) ; \
	rm -f *.a *.o
	cd $(DATASTRUCTURES_DIR) ; \
	rm -f *.a *.o fftw.*
	cd $(FILTER_DIR) ; \
	rm -f *.a *.o
	cd $(GEOMETRY_DIR) ; \
	rm -f *.a *.o
	cd $(IMAGE_DIR) ; \
	rm -f *.a *.o
	cd $(KDTREE_DIR) ; \
	rm -f *.a *.o
	cd $(LADAR_DIR) ; \
	rm -f *.a *.o
	cd $(MATH_DIR) ; \
	rm -f *.a *.o
	cd $(NETWORK_DIR) ; \
	rm -f *.a *.o
	cd $(PLOT_DIR) ; \
	rm -f *.a *.o
	cd $(THREEDGRAPHICS_DIR) ; \
	rm -f *.a *.o
	cd $(URBAN_DIR) ; \
	rm -f *.a *.o

